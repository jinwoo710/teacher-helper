{
  "name": "teacher-helper",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "wrangler pages deploy dist",
    "db:migrate": "drizzle-kit generate",
    "db:midrate:local": "wrangler d1 migrations apply teacher-helper-db --local",
    "db:midrate:prod": "wrangler d1 migrations apply teacher-helper-db --remote"
  },
  "dependencies": {
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.40.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250311.0",
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.12",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.12",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "wrangler": "^3.114.0"
  }
}
